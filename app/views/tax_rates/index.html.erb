<div style="float:left">
	<%= render 'shared/side_menu' %>
</div>
<div>
	<div class="row">
		<div class="span12 offset3" style="min-height:500px">
			<div class="datatable_container">
				<div class="datatable_head">
					<h3>Manage Taxes</h3>
				</div>
				<div class="datatable_row datatable_row_line">
					<div class="datatable_column datatable_headings">
						Description
					</div>
					<div class="datatable_column datatable_headings">
						Factor
					</div>
					<div class="datatable_column datatable_headings">
						Valid From
					</div>
					<div class="datatable_column datatable_headings">
						Valid Until
					</div>
					<div class="datatable_column datatable_headings">
						Is Default
					</div>
					<div class="datatable_column datatable_headings">
						Action
					</div>

				</div>

				<% @tax_rate.each do |tax_rate| %>
				<div class="datatable_row_clear">
				</div>
				<div class="datatable_row">
					<div class="datatable_column">
						<%= tax_rate.description %>
					</div>
					<div class="datatable_column">
						<%= tax_rate.factor %>
					</div>
					<div class="datatable_column">
						<%= tax_rate.valid_from %>
					</div>
					<div class="datatable_column">
						<%= tax_rate.valid_until %>
					</div>
					<div class="datatable_column">
						<%= tax_rate.is_default %>
					</div>
					<div class="datatable_column">
						<%= link_to t('.edit', :default => t("helpers.links.edit")),
						edit_tax_rate_path(tax_rate), :class => 'btn btn-mini' %>

						<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
						tax_rate_path(tax_rate),
						:method => :delete,
						:data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
						:class => 'btn btn-mini btn-danger' %>
					</div>
				</div>

				<% end %>
			</div>
		</div>
	</div>
</div>
  <div class='apple_pagination' align="center">
    
     <%= will_paginate @tax_rate, :container => false %>
   </div>
